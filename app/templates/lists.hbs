<input type="text" id="todo-list" placeholder="What needs to be done?" />

<section id="main">
    <ul id="todo-list">
        {{#each model as |list|}}
            <li class="completed">
                <input type="checkbox" class="toggle">
                <label>{{list.title}}</label><button class="destroy"></button>
            </li>
        {{/each}}
    </ul>

    <input type="checkbox" id="toggle-all">
</section>

<footer id="footer">
    <span id="todo-count">
        <strong>2</strong> lists left
    </span>
    <ul id="filters">
        <li>
            <a href="all" class="selected">All</a>
        </li>
        <li>
            <a href="active">Active</a>
        </li>
        <li>
            <a href="completed">Completed</a>
        </li>
    </ul>

    <button id="clear-completed">
        Clear completed (1)
    </button>
</footer>
